        {
          "type": "character",
          "attributes": {},
          "value": ["function(bs)\n{\n    # log-likihood fitting as a risk function\n    fit_routine <- function(par, x)\n    {\n        # compute risk\n        p <- dweibull(x, shape = par[\"shape\"], scale = par[\"scale\"])\n        mean(-log(p))\n    }\n\n    # fit weibull to data\n    fit <- optim(\n        par    = c(shape = 1, scale = mean(bs)), \n        fn     = fit_routine, \n        gr     = NULL,\n        bs, \n        method = \"L-BFGS-B\", \n        lower  = c(shape = 1e-06, scale = 0)\n    )\n\n    # return results as named list\n    # return values are matched to values of bootstrap result vars\n    list(bs_scale_uncert = fit$par[\"scale\"], \n         bs_shape_uncert = fit$par[\"shape\"])\n}"]
